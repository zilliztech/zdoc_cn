        "text": {
          "elements": [
            {
              "text_run": {
                "content": "What is a collection alias?",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
