            },
            {
              "text_run": {
                "content": ") to this collection by calling ",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            },
            {
              "text_run": {
                "content": "create_alias(\"collection_",
