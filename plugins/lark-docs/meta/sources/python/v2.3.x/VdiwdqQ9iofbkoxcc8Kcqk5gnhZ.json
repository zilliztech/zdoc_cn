        "text": {
          "elements": [
            {
              "text_run": {
                "content": "Continuously calling this operation results in many sealed segments of small sizes, which can gradually degrade search performance. ",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
