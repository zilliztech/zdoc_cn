        "text": {
          "elements": [
            {
              "text_run": {
                "content": "When new data is inserted, it is written into a growing segment. Once the size of a growing segment reaches its upper limit, <include target=\"milvus\">Milvus</include><include target=\"zilliz\">Zilliz Cloud</include> automatically seals the segment. ",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            }
