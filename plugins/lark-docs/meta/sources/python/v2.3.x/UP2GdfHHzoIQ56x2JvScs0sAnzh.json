      {
        "block_id": "Z8RIdodXCoT2wrxCUpgcJrFBnoe",
        "block_type": 14,
        "code": {
          "elements": [
            {
              "text_run": {
                "content": "from pymilvus import MilvusClient\n\n# 1. Create a milvus client\nclient = MilvusClient(\n    uri=\"http://localhost:19530\",\n    token=\"root:Milvus\"\n)\n\nread_only_privileges = [\n  {\"object_type\": \"Global\", \"object_name\": \"*\", \"privilege\": \"DescribeCollection\"},\n  {\"object_type\": \"Global\", \"object_name\": \"*\", \"privilege\": \"ShowCollections\"},\n  {\"object_type\": \"Collection\", \"object_name\": \"*\", \"privilege\": \"Search\"},\n  {\"object_type\": \"Collection\", \"object_name\": \"*\", \"privilege\": \"Query\"},\n] \n\n# 2. Create a role\nclient.create_role(role_name=\"read_only\")\n\n# 3. Grant privileges\nfor item in read_only_privileges:\n",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            },
            {
              "text_run": {
                "content": "    client.grant_pri",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            },
            {
              "text_run": {
                "content": "vilege(",
                "text_element_style": {
                  "bold": false,
                  "comment_ids": [
                    "7330927868912467971"
                  ],
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            },
            {
              "text_run": {
                "content": "\n        role_name=\"read_only\",\n        object_type=item[\"object_type\"],\n        privilege=item[\"privilege\"],\n        object_name=item[\"object_name\"]\n    )\n    \n# 4. Revoke privilege\nclient.revoke_privilege(\n    role_name=\"read_only\",\n    object_type=\"Global\",\n    privilege=\"*\",\n    object_name=\"DescribeCollection\"\n",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
