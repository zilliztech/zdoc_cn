        "block_type": 2,
        "parent_id": "K20ndD4WZoCjnIxfVmDclQodnpf",
        "text": {
          "elements": [
            {
              "text_run": {
                "content": "What is a replica?",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            }
