      {
        "block_id": "doxcnnNuGPDcjD4dhgK1ogaoC6m",
        "block_type": 14,
        "code": {
          "elements": [
            {
              "text_run": {
                "content": "from pymilvus import (\n    connections, \n    Collection, \n    FieldSchema, \n    CollectionSchema, \n    DataType, \n    utility,\n)\n\n# Connect to localhost:19530\nconnections.connect()\n\n# Create a collection\ncollection = Collection(\n    name=\"test_collection_flush\", \n    schema=CollectionSchema(fields=[\n        FieldSchema(\"film_id\", DataType.INT64, is_primary=True),\n        FieldSchema(\"films\", dtype=DataType.FLOAT_VECTOR, dim=128)\n    ])\n)\n\n# Insert data\ncollection.insert([[1, 2], [[1.0, 2.0], [3.0, 4.0]]])\n\nutility.flush_all(_async=False) # synchronized flush_all\n# or use `future` to flush_all asynchronously\n\nfuture = utility.flush_all(_async=True)\n",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            },
            {
              "text_run": {
                "content": "future.done() # flush_all finished",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            }
          ],
