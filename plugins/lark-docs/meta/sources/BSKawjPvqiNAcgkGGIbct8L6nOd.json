      {
        "block_id": "VqakdB8NqoHNK1xhqb9cj3fZnfh",
        "block_type": 14,
        "code": {
          "elements": [
            {
              "text_run": {
                "content": "from os import environ\n\nHOST = \"<instance-id>.<cloud-region-id>.vectordb.zillizcloud.com\" # Cluster 公共端点主机名，从 Zilliz Cloud 上获取\nPORT = 443  # Cluster 公共端点端口\n\nUSER = \"db_admin\" # 访问 Cluster 的用户名\nPASSWORD = \"***\"  # 上述用户名对应的密码\n\n",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            },
            {
              "text_run": {
                "content": "environ[\"OPENAI_API_KEY\"] = \"sk-******\" # OpenAI API 密钥",
                "text_element_style": {
