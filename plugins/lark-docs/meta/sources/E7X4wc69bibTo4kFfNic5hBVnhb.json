        "text": {
          "elements": [
            {
              "text_run": {
                "content": "一旦账户准备就绪，您便可以登录 Zilliz Cloud 控制台以创建和运行集群。",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            }
          ],
          "style": {
            "align": 1,
            "folded": false
