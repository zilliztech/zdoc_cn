        "text": {
          "elements": [
            {
              "text_run": {
                "content": "轻松实时导入数据并创建索引。确保最新内容立即可用于所有搜索查询。",
                "text_element_style": {
                  "bold": false,
                  "inline_code": false,
                  "italic": false,
                  "strikethrough": false,
                  "underline": false
                }
              }
            }
          ],
          "style": {
            "align": 1,
            "folded": false
          }
        }
      },
      {
